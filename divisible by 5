start_range = 1
end_range = 100
num_divisible_by_5 = 0
sum_divisible_by_5 = 0
for num in range(start_range, end_range + 1):
    if num % 5 == 0:
        num_divisible_by_5 += 1
        sum_divisible_by_5 += num

print(f"Number of integers divisible by 5: {num_divisible_by_5}")
print(f"Sum of integers divisible by 5: {sum_divisible_by_5}")
